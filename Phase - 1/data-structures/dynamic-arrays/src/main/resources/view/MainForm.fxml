            <Button fx:id="btnRemove" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnRemoveOnAction" text="REMOVE" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox layoutX="50.0" layoutY="245.0" maxWidth="1.7976931348623157E308" spacing="30.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
         <children>
            <Button fx:id="btnContains" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnContainsOnAction" text="CONTAINS" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnSize" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnSizeOnAction" text="SIZE" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnClear" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnClearOnAction" text="CLEAR" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</AnchorPane>
